generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL") 
}

model Episode {
  id Int @id @default(autoincrement())
  description String @db.Text
  duration_ms Int
  href String
  html_description String @db.Text
  name String
  release_date String
  image String
  spotify_id String
  audio_preview_url String
}